<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestTemas3Fragment"
    android:background="#1F347F">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="25dp"
        android:layout_marginTop="76dp"
        android:src="@drawable/logocoche"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <LinearLayout
        android:id="@+id/senales"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginLeft="20dp"

        android:layout_marginTop="72dp"
        android:layout_marginRight="20dp"
        android:background="#2207BA"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.028"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:src="@drawable/logofactores" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="202dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_horizontal"
            android:text="Factores Humanos"
            android:textColor="#FFFCFC"

            android:textSize="20dp"
            tools:ignore="MissingConstraints" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/senales2"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginLeft="20dp"

        android:layout_marginRight="20dp"
        android:background="#2207BA"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/senales"
        app:layout_constraintVertical_bias="0.236"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/imageView81"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:src="@drawable/logosenal" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="202dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_horizontal"
            android:text="La Señalización"
            android:textColor="#FFFCFC"

            android:textSize="20dp"
            tools:ignore="MissingConstraints" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/senales3"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginLeft="20dp"

        android:layout_marginRight="20dp"
        android:background="#2207BA"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/senales2"
        app:layout_constraintVertical_bias="0.498"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/imageView38"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:src="@drawable/logodocumentos" />

        <TextView
            android:id="@+id/textView52"
            android:layout_width="202dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_horizontal"
            android:text="DEFINICIONES Y DOCUMENTACIÓN"
            android:textColor="#FFFCFC"

            android:textSize="20dp"
            tools:ignore="MissingConstraints" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>